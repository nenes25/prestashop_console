{
    "chmod": "0755",
    "directories": [
        "src/PrestashopConsole"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": ["Tests"],
            "in": "src/vendor"
        },
        {
            "name": "config.php",
            "in":"."
        }
    ],
    "main": "phar/consolePhar.php",
    "output": "phar/prestashopConsole.phar",
    "stub": true,
    "banner" : [
        "PrestashopConsole Project",
        "@link https://github.com/nenes25/prestashop_console"
    ],
    "compression" : "GZ"
}
